@model AuktionsOpgave.Models.Person

@{
    ViewBag.Title = "Login";
}



    <div class="jumbotron" style="text-align:center">
        @using (Html.BeginForm())
        {
            @Html.Label("Email")
            @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control", @style = "margin-left:36%;margin-right:auto; margin-bottom:10px" } })
            @Html.Label("Password")
            @Html.EditorFor(model => model.Password, new { htmlAttributes = new { @class = "form-control", @style = "margin-left:36%;margin-right:auto; margin-bottom:10px" } })
            <input type="submit" value="Login" class="btn btn-success btn-lg">

        }
    </div>
